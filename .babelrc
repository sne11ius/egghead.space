{
  "presets": [
    ["env", { "modules": false } ],
    ["stage-2"]
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-runtime",
    ["transform-imports", {
      "vuetify": {
        "transform": "vuetify/es5/components/${member}",
        "preventFullImport": true
      }
    }]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
